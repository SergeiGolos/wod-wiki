{
  "name": "wod-wiki",
  "version": "0.5.0",
  "description": "A React component library for parsing, displaying, and executing workout definitions using a specialized syntax. Features include a Monaco Editor integration for editing workout scripts, a runtime engine for execution, and components styled with Tailwind CSS.",
  "homepage": "https://github.com/SergeiGolos/wod-wiki#readme",
  "bugs": {
    "url": "https://github.com/SergeiGolos/wod-wiki/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SergeiGolos/wod-wiki.git"
  },
  "license": "ISC",
  "author": "",
  "type": "commonjs",
  "main": "index.js",
  "scripts": {
    "setup": "node setup-testing.js",
    "test": "vitest",
    "test:unit": "vitest run -c vitest.unit.config.js",
    "test:storybook": "vitest run --project=storybook",
    "test:watch": "vitest --project=unit",
    "test:storybook:watch": "vitest --project=storybook",
    "test:e2e": "npx playwright test",
    "prestorybook": "dotenv -e .env -- node scripts/inject-ga-id.js",
    "storybook": "npm run prestorybook && storybook dev -p 6006",
    "storybook:debug": "npm run prestorybook && set NODE_OPTIONS=--inspect=9229 && storybook dev -p 6006",
    "prebuild-storybook": "dotenv -e .env -- node scripts/inject-ga-id.js",
    "build-storybook": "npm run storybook build"
  },
  "dependencies": {
    "@monaco-editor/react": "^4.7.0",
    "@playwright/test": "^1.53.1",
    "chevrotain": "^11.0.3"
  },
  "optionalDependencies": {
    "@rollup/rollup-win32-x64-msvc": "^4.43.0"
  },
  "devDependencies": {
    "@storybook/addon-a11y": "^9.1.1",
    "@storybook/addon-docs": "^9.1.1",
    "@storybook/addon-vitest": "^9.1.1",
    "@storybook/react-vite": "^9.1.1",
    "@types/node": "^22.0.0",
    "@vitest/browser": "^3.2.4",
    "@vitest/coverage-v8": "^3.2.4",
    "autoprefixer": "^10.4.21",
    "dotenv-cli": "^8.0.0",
    "playwright": "^1.53.1",
    "postcss": "^8.5.6",
    "prop-types": "^15.8.1",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "rollup": "^4.41.0",
    "rxjs": "^7.8.2",
    "storybook": "^9.1.1",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.0.0",
    "vite": "^6.3.5",
    "vitest": "^3.2.4"
  }
}
