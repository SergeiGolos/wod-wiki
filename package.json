{
  "name": "wod-wiki",
  "version": "0.5.0",
  "description": "A React component library for parsing, displaying, and executing workout definitions using a specialized syntax. Features include a Monaco Editor integration for editing workout scripts, a runtime engine for execution, and components styled with Tailwind CSS.",
  "homepage": "https://github.com/SergeiGolos/wod-wiki#readme",
  "bugs": {
    "url": "https://github.com/SergeiGolos/wod-wiki/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SergeiGolos/wod-wiki.git"
  },
  "scripts": {
    "setup": "node setup-testing.js",
    "test": "vitest run -c vitest.unit.config.js",
    "test:watch": "vitest -c vitest.unit.config.js",
    "test:coverage": "vitest run -c vitest.unit.config.js --coverage --coverage.reporter=html --coverage.reporter=json-summary",
    "test:ui": "vitest run -c vitest.storybook.config.js && npm run test:e2e",
    "test:perf": "vitest run tests/performance/**/*.test.ts",
    "test:e2e": "npx playwright test",
    "playwright": "npx playwright test",
    "build:lib": "vite build",
    "build": "npm run build-storybook",
    "prestorybook": "npx dotenv-cli -e .env -- node scripts/inject-ga-id.cjs",
    "storybook": "npm run prestorybook && storybook dev -p 6006",
    "storybook:debug": "npm run prestorybook && set NODE_OPTIONS=--inspect=9229 && storybook dev -p 6006",
    "clean-storybook": "node -e \"require('fs').rmSync('storybook-static', { recursive: true, force: true })\"",
    "prebuild-storybook": "npm run clean-storybook && npx dotenv-cli -e .env -- node scripts/inject-ga-id.cjs",
    "build-storybook": "npm run prestorybook && storybook build",
    "docs:check": "node scripts/check-docs-links.cjs",
    "dev:all": "node scripts/dev-start.cjs",
    "dev:web": "node scripts/dev-start.cjs --web-only",
    "dev:no-emulator": "node scripts/dev-start.cjs --no-emulator"
  },
  "dependencies": {
    "@headlessui/react": "^2.2.9",
    "@monaco-editor/react": "^4.6.0 || ^4.7.0",
    "@phosphor-icons/react": "^2.1.10",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "chevrotain": "^11.0.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "lucide-react": "^0.545.0",
    "pnpm": "^10.20.0",
    "react-markdown": "^10.1.0",
    "recharts": "^3.4.1",
    "sharp": "^0.34.4",
    "tailwind-merge": "^3.3.1",
    "uuid": "^13.0.0"
  },
  "peerDependencies": {
    "@monaco-editor/react": "^4.6.0 || ^4.7.0",
    "monaco-editor": "^0.44.0 || ^0.45.0 || ^0.46.0 || ^0.47.0 || ^0.48.0 || ^0.49.0 || ^0.50.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "peerDependenciesMeta": {
    "monaco-editor": {
      "optional": true
    },
    "@monaco-editor/react": {
      "optional": true
    }
  },
  "optionalDependencies": {
    "@rollup/rollup-win32-x64-msvc": "^4.43.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.57.0",
    "@storybook/addon-a11y": "^10.1.2",
    "@storybook/addon-docs": "^10.1.2",
    "@storybook/addon-vitest": "^10.1.2",
    "@storybook/react-vite": "^10.1.2",
    "@testing-library/react": "^16.3.0",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.5",
    "@types/node": "^22.0.0",
    "@types/uuid": "^10.0.0",
    "@vitejs/plugin-react": "^4.3.1",
    "@vitest/browser": "^3.2.4",
    "@vitest/coverage-v8": "^3.2.4",
    "autoprefixer": "^10.4.21",
    "concurrently": "^9.2.1",
    "cors": "^2.8.5",
    "dotenv-cli": "^8.0.0",
    "express": "^5.1.0",
    "jsdom": "^27.0.0",
    "playwright": "^1.57.0",
    "postcss": "^8.5.6",
    "prop-types": "^15.8.1",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "rollup": "^4.41.0",
    "rxjs": "^7.8.2",
    "storybook": "^10.1.2",
    "tailwindcss": "^3.4.17",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "^5.0.0",
    "vite": "^6.4.1",
    "vite-plugin-dts": "^4.5.4",
    "vitest": "^3.2.4"
  }
}
