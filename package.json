{
  "name": "wod-wiki",
  "version": "0.5.0",
  "description": "A React component library for parsing, displaying, and executing workout definitions using a specialized syntax. Features include a Monaco Editor integration for editing workout scripts, a runtime engine for execution, and components styled with Tailwind CSS.",
  "homepage": "https://github.com/SergeiGolos/wod-wiki#readme",
  "bugs": {
    "url": "https://github.com/SergeiGolos/wod-wiki/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SergeiGolos/wod-wiki.git"
  },
  "scripts": {
    "setup": "bun setup-testing.js",
    "test": "bun test src --preload ./tests/unit-setup.ts",
    "test:components": "bun test tests --preload ./tests/setup.ts",
    "test:all": "bun run test && bun run test:components",
    "test:coverage": "bun test src --preload ./tests/unit-setup.ts --coverage --coverage-reporter=lcov",
    "test:storybook": "bunx vitest run --config vitest.storybook.config.js",
    "test:ui": "bun run test:storybook && bun run test:e2e",
    "test:perf": "bun test tests/performance",
    "test:e2e": "bun x playwright test",
    "build:lib": "vite build",
    "build:app": "bun x vite build --config vite.app.config.ts",
    "preview:app": "bun x vite preview --config vite.app.config.ts",
    "build": "bun run build-storybook",
    "storybook": "npx storybook dev -p 6006",
    "storybook:debug": "set NODE_OPTIONS=--inspect=9229 && npx storybook dev -p 6006",
    "clean-storybook": "bun -e 'require(\"fs\").rmSync(\"storybook-static\", { recursive: true, force: true })'",
    "prebuild-storybook": "bun run clean-storybook",
    "build-storybook": "NODE_OPTIONS=--max-old-space-size=4096 storybook build",
    "docs:check": "bun scripts/check-docs-links.cjs",
    "dev": "bun scripts/dev-start.cjs",
    "dev:app": "bun x vite --config vite.app.config.ts"
  },
  "dependencies": {
    "@codemirror/autocomplete": "^6.20.0",
    "@codemirror/commands": "^6.10.2",
    "@codemirror/lang-markdown": "^6.5.0",
    "@codemirror/language": "^6.12.2",
    "@codemirror/lint": "^6.9.4",
    "@codemirror/search": "^6.6.0",
    "@codemirror/state": "^6.5.4",
    "@codemirror/theme-one-dark": "^6.1.3",
    "@codemirror/view": "^6.39.15",
    "@headlessui/react": "^2.2.9",
    "@lezer/common": "^1.5.1",
    "@lezer/highlight": "^1.2.3",
    "@lezer/lr": "^1.4.8",
    "@phosphor-icons/react": "^2.1.10",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@types/jszip": "^3.4.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "idb": "^8.0.3",
    "jszip": "^3.10.1",
    "lucide-react": "^0.545.0",
    "react-joyride": "^2.9.3",
    "react-markdown": "^10.1.0",
    "react-router-dom": "^7.13.0",
    "recharts": "^3.4.1",
    "tailwind-merge": "^3.3.1",
    "uuid": "^13.0.0",
    "zustand": "^5.0.11"
  },
  "peerDependencies": {
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "peerDependenciesMeta": {
    "monaco-editor": {
      "optional": true
    },
    "@monaco-editor/react": {
      "optional": true
    }
  },
  "optionalDependencies": {
    "@rollup/rollup-win32-x64-msvc": "^4.43.0"
  },
  "devDependencies": {
    "@lezer/generator": "^1.8.0",
    "@playwright/test": "^1.57.0",
    "@storybook/addon-a11y": "^10.2.13",
    "@storybook/addon-docs": "^10.2.13",
    "@storybook/addon-vitest": "^10.2.13",
    "@storybook/react-vite": "^10.2.13",
    "@testing-library/react": "^16.3.0",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.5",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^22.0.0",
    "@types/react-dom": "^19.2.3",
    "@types/uuid": "^10.0.0",
    "@vitejs/plugin-react": "^4.3.1",
    "@vitest/browser": "3.2.4",
    "@vitest/coverage-v8": "^3.2.4",
    "autoprefixer": "^10.4.21",
    "bun-types": "^1.2.0",
    "concurrently": "^9.2.1",
    "cors": "^2.8.5",
    "dotenv-cli": "^8.0.0",
    "eslint": "^9.39.1",
    "eslint-plugin-storybook": "10.2.13",
    "express": "^5.1.0",
    "jsdom": "^27.4.0",
    "playwright": "^1.57.0",
    "postcss": "^8.5.6",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "rollup": "^4.41.0",
    "storybook": "^10.2.13",
    "tailwindcss": "^3.4.17",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "^5.0.0",
    "vite": "^6.4.1",
    "vite-plugin-dts": "^4.5.4",
    "vitest": "^3.2.4"
  }
}